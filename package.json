{
  "name": "@guilhermeabreudev/fetch-stack",
  "version": "1.0.1",
  "description": "Uma biblioteca simples e eficiente para gerenciamento de estado e cache de dados, inspirada no React Query",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:examples",
    "build:lib": "tsc",
    "build:examples": "tsc -p tsconfig.examples.json",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build:lib",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuilhermeAbreu/fetch-stack.git"
  },
  "keywords": [
    "state-management",
    "data-fetching",
    "cache",
    "typescript",
    "react-query"
  ],
  "author": "Guilherme Abreu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GuilhermeAbreu/fetch-stack/issues"
  },
  "homepage": "https://github.com/GuilhermeAbreu/fetch-stack#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lru-cache": "^5.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
